doctype 5
html
  head
    title= title
    link(rel='stylesheet', href='/css/style.css')
    link(href='http://fonts.googleapis.com/css?family=Lora:400,700', rel='stylesheet')
  body
    block content

    script
      var appConfig = {
        roomKey: #{roomKey},
        roomLink: '#{roomLink}',
        userId: #{userId},
        initiator: #{initiator},
        pcConfig: JSON.parse('!{pcConfig}'),
        pcConstraints: JSON.parse('!{pcConstraints}'),
        mediaConstraints: JSON.parse('!{mediaConstraints}')
      };

    script(src="http://code.jquery.com/jquery-1.9.1.min.js")
    script(src="/js/adapter.js")
    script(src='/js/socket.io.js')
    script(src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDPLye9CJqotI26gD6WSqCbyyONy8aTNms&sensor=false")
    script(src="/js/custom.js")
    script(src="/js/webrtc-demo.js")